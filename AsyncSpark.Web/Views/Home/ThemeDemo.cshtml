    <section class="mb-5" id="typography">
        <div class="card">
            <div class="card-header d-flex justify-content-between align-items-center">
                <h2 class="mb-0"><i class="bi bi-fonts me-2"></i>Typography</h2>
                <button class="btn btn-sm btn-outline-secondary" type="button" data-bs-toggle="collapse" data-bs-target="#codeTypography">
                    <i class="bi bi-code-slash me-1"></i>Code
                </button>
            </div>
            <div class="card-body">
                <div class="row g-4">
                    <div class="col-md-6">
                        <h1>h1. Bootstrap heading</h1>
                        <h2>h2. Bootstrap heading</h2>
                        <h3>h3. Bootstrap heading</h3>
                        <h4>h4. Bootstrap heading</h4>
                        <h5>h5. Bootstrap heading</h5>
                        <h6>h6. Bootstrap heading</h6>
                    </div>
                    <div class="col-md-6">
                        <p>Default paragraph text with <a href="#">an inline link</a>.</p>
                        <p class="lead">Lead paragraph text</p>
                        <p class="text-body-secondary">Secondary text (text-body-secondary)</p>
                        <p><mark>Highlighted text</mark></p>
                        <p><small>Small text</small></p>
                        <p><strong>Bold text</strong> and <em>italic text</em></p>
                        <blockquote class="blockquote">
                            <p>A blockquote element.</p>
                        </blockquote>
                        <figcaption class="blockquote-footer">
                            Someone famous in <cite title="Source Title">Source Title</cite>
                        </figcaption>
