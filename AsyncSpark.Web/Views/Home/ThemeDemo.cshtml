                <div>
                    <button type="button" class="btn btn-primary btn-lg me-2 mb-2">Large</button>
                    <button type="button" class="btn btn-primary me-2 mb-2">Default</button>
                    <button type="button" class="btn btn-primary btn-sm me-2 mb-2">Small</button>
                    <button type="button" class="btn btn-primary active me-2 mb-2">Active</button>
                    <button type="button" class="btn btn-primary mb-2" disabled>Disabled</button>
                </div>
